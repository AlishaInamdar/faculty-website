* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

body {
  font-family: 'Segoe UI', sans-serif;
  background-color: #0d0d0d;
  color: #fff;
  overflow-x: hidden;
}

/* Header */
.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #121212;
  padding: 20px 60px;
  position: sticky;
  top: 0;
  z-index: 100;
}

.logo {
  font-size: 24px;
  font-weight: bold;
  color:white;
}

nav a {
  margin-left: 20px;
  text-decoration: none;
  color: #ccc;
  font-size: 15px;
  transition: color 0.3s ease;
}

nav a:hover,
nav a.active {
  color: #00bcd4;
}

/* Project Section */
.projects-section {
  position: relative;
  padding: 80px 20px;
  background: url('https://images.unsplash.com/photo-1581090700227-1e8eab02=
1d95?auto=3Dformat&amp;fit=3Dcrop&amp;w=3D1600&amp;q=3D80') no-repeat cente=
r center/cover;
  min-height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}


.overlay {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.75);
  z-index: 1;
}

.projects-container {
  position: relative;
  z-index: 2;
  max-width: 800px;
  text-align: center;
}

.projects-container h1 {
  font-size: 36px;
  margin-bottom: 10px;
  color: #00bcd4;
}

.projects-container p {
  margin-bottom: 40px;
  color: #ccc;
  font-size: 16px;
}

/* Project Cards */
.project-card {
  background-color: rgba(255, 255, 255, 0.05);
  padding: 20px;
  margin-bottom: 25px;
  border-radius: 12px;
  box-shadow: 0 8px 20px rgba(0, 188, 212, 0.1);
  transition: transform 0.3s;
  text-align: left;
}

.project-card h3 {
  color: #00bcd4;
  margin-bottom: 10px;
  font-size: 18px;
}

.project-card p {
  font-size: 14px;
  color: #ddd;
}

.project-card:hover {
  transform: translateY(-5px);
}
/* Two-column layout */
.split-layout {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  z-index: 2;
  position: relative;
  gap: 40px;
  margin-top: -30px;
}

/* Left: Projects */
.projects-container {
  flex: 1;
  min-width: 300px;
  padding: 20px;
}

/* Right: Papers Published */
.papers-container {
  flex: 1;
  min-width: 300px;
  padding: 20px;
}

.papers-container h1 {
  font-size: 36px;
  margin-bottom: 30px;
  color: #00ffc3;
}

.paper-list {
  display: flex;
  flex-direction: column;
  gap: 25px;
}

.paper-card {
  background: rgba(255, 255, 255, 0.05);
  padding: 20px;
  border-radius: 12px;
  backdrop-filter: blur(5px);
  transition: 0.3s;
  box-shadow: 0 6px 15px rgba(0, 255, 196, 0.1);
}

.paper-card a {
  text-decoration: none;
  color: inherit;
}
.paper-card a:hover p {
  color: #00ffc3;
  text-decoration: underline;
}


.paper-card:hover {
  transform: translateY(-5px);
  background: rgba(0, 255, 195, 0.07);
}

.paper-card h3 {
  color: #00ffc3;
  margin-bottom: 10px;
  font-size: 17px;
}

.paper-card p {
  color: #ddd;
  font-size: 14px;
}
